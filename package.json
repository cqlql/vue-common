{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:analyzer": "vite build --mode analyzer",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "ant-design-vue": "^4.1.2",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.23",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-core": "^41.1.0",
    "@ckeditor/ckeditor5-editor-classic": "^41.1.0",
    "@ckeditor/ckeditor5-image": "^41.1.0",
    "@ckeditor/ckeditor5-watchdog": "^41.1.0",
    "@iconify/json": "^2.2.201",
    "@rushstack/eslint-patch": "^1.10.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.7",
    "@typescript-nameof/common-types": "^0.0.7",
    "@typescript-nameof/types": "^0.0.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.7.3",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-vue": "^9.25.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "postcss-html": "^1.6.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "start-server-and-test": "^2.0.3",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended": "^13.0.0",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.9",
    "vite-bundle-analyzer": "^0.9.4",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-ts-nameof": "^3.0.0",
    "vite-plugin-vue-devtools": "^7.0.27",
    "vitest": "^1.5.0",
    "vue-tsc": "^2.0.13"
  }
}